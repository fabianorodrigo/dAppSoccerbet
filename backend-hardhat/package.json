{
  "name": "dappsoccerbet-backend-hardhat",
  "version": "0.5.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "echo \"Error: no start specified\" && exit 1",
    "clean": "npx hardhat clean",
    "build": "npx hardhat compile",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage",
    "trailofbits": "docker run -it -v \"$PWD\":/home/trufflecon trailofbits/eth-security-toolbox",
    "slither": "docker run --entrypoint=\"\" --rm --name dappsoccerapp_cicd -v $PWD:/home/dappsoccerbet/repo/ --user $(id -u):$(id -g) fabianorodrigo/dappsoccerbet_hardhat:1.0.4 slither . --exclude-dependencies --exclude-informational --exclude-low",
    "slither-check-erc": "docker run --entrypoint=\"\" --rm --name dappsoccerapp_cicd -v $PWD:/home/dappsoccerbet/repo/ --user $(id -u):$(id -g) fabianorodrigo/dappsoccerbet_hardhat:1.0.4 slither-check-erc . BetTokenUpgradeable",
    "slither-check-upgradeability-bettoken": "docker run --entrypoint=\"\" --rm --name dappsoccerapp_cicd -v $PWD:/home/dappsoccerbet/repo/ --user $(id -u):$(id -g) fabianorodrigo/dappsoccerbet_hardhat:1.0.4  slither-check-upgradeability . BetTokenUpgradeable",
    "slither-check-upgradeability-gamefactory": "docker run --entrypoint=\"\" --rm --name dappsoccerapp_cicd -v $PWD:/home/dappsoccerbet/repo/ --user $(id -u):$(id -g) fabianorodrigo/dappsoccerbet_hardhat:1.0.4  slither-check-upgradeability . GameFactoryUpgradeable",
    "slither-check-upgradeability-calculator": "docker run --entrypoint=\"\" --rm --name dappsoccerapp_cicd -v $PWD:/home/dappsoccerbet/repo/ --user $(id -u):$(id -g) fabianorodrigo/dappsoccerbet_hardhat:1.0.4  slither-check-upgradeability . CalculatorUpgradeable",
    "slither-prop": "docker run --entrypoint=\"\" --rm --name dappsoccerapp_cicd -v $PWD:/home/dappsoccerbet/repo/ --user $(id -u):$(id -g) fabianorodrigo/dappsoccerbet_hardhat:1.0.4 slither-prop . --contract Game",
    "slither-summary": "docker run --entrypoint=\"\" --rm --name dappsoccerapp_cicd -v $PWD:/home/dappsoccerbet/repo/ --user $(id -u):$(id -g) fabianorodrigo/dappsoccerbet_hardhat:1.0.4 slither . --print function-summary",
    "slither-inheritance": "docker run --entrypoint=\"\" --rm --name dappsoccerapp_cicd -v $PWD:/home/dappsoccerbet/repo/ --user $(id -u):$(id -g) fabianorodrigo/dappsoccerbet_hardhat:1.0.4 slither . --print inheritance-graph",
    "slither-statevars": "docker run --entrypoint=\"\" --rm --name dappsoccerapp_cicd -v $PWD:/home/dappsoccerbet/repo/ --user $(id -u):$(id -g) fabianorodrigo/dappsoccerbet_hardhat:1.0.4 slither . --print vars-and-auth",
    "manticore": "docker run --entrypoint=\"\" --rm --name dappsoccerapp_cicd -v $PWD:/home/dappsoccerbet/repo/ --user $(id -u):$(id -g) fabianorodrigo/dappsoccerbet_hardhat:1.0.4 manticore .",
    "ganache": "ganache -p 8545 -i 31337 --chain.chainId 31337 --wallet.deterministic --database.dbPath ~/sgbds/ganache --miner.blockTime 10 ",
    "ganache-debug": "ganache -p 8545 -i 31337 --chain.chainId 31337 --wallet.deterministic --database.dbPath ~/sgbds/ganache --logging.debug --logging.verbose ",
    "hardhatnode": "npx hardhat node",
    "deployLocal": "npx hardhat run --network localhost scripts/deployProxies.ts",
    "upgradeBetTokenLocal": "npx hardhat run --network localhost scripts/upgradeBetToken.ts",
    "upgradeGameFactoryLocal": "npx hardhat run --network localhost scripts/upgradeGameFactory.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.33",
    "chai": "^4.3.6",
    "ethereum-waffle": "^3.4.4",
    "ethers": "5.6.1",
    "hardhat": "^2.9.5",
    "hardhat-gas-reporter": "^1.0.8",
    "mocha-junit-reporter": "^2.0.2",
    "mocha-multi-reporters": "^1.5.1",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.6.0",
    "@openzeppelin/contracts-upgradeable": "^4.6.0",
    "solidity-coverage": "^0.7.21"
  }
}
